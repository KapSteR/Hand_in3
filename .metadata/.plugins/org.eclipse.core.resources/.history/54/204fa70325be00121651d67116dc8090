package com.kn10731.iha.handin4;

public class Costumers {

	// private variables
	int _id;
	String _name;
	String _address;

	// Empty constructor
	public Costumer() {

	}

	// constructor
	public Costumer(int id, String name, String _address) {
		this._id = id;
		this._name = name;
		this._address = _address;
	}

	// constructor
	public Contact(String name, String _address) {
		this._name = name;
		this._address = _address;
	}

	// getting ID
	public int getID() {
		return this._id;
	}

	// setting id
	public void setID(int id) {
		this._id = id;
	}

	// getting name
	public String getName() {
		return this._name;
	}

	// setting name
	public void setName(String name) {
		this._name = name;
	}

	// getting phone number
	public String getAddress() {
		return this._address;
	}

	// setting phone number
	public void setPhoneNumber(String phone_number) {
		this._phone_number = phone_number;
	}
}

